meme_dict = {
            "CRINGE": "Garip ya da utandırıcı bir şey",
            "LOL": "Komik bir şeye verilen cevap",
            "IDK": "Bir şeyi bilmediğimiz zaman söylenen cevap",
            "BTW": "İngilizcede bu arada anlamına gelen kalıpıtır",
            "GG": "Bir takım kazandığı zaman söylenen,iyi oyundu anlamına gelen sözdür.",
            "VIP": "Çok Önemli Kişi anlamına gelir",
            "TY": "Teşekkürler anlamına gelir.", 
            "AFK": "Oyunda değil demektir",
            "GOAT": "Tüm zamanların en iyisi demektir",
            "IDC": "Umursamıyorum demektir",
            }

word = input("Anlamadığınız bir kelime yazınız")
if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("Aradığınız kelime sözlükte bulunmamaktadır.")
